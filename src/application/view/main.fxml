<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="800.0" maxWidth="800.0" prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.mainController">
   <children>
      <Label fx:id="welcomeLabel" layoutX="25.0" layoutY="15.0" prefHeight="85.0" prefWidth="650.0" text="Alex: Welcome to the Food Checker App">
         <font>
            <Font size="35.0" />
         </font>
      </Label>
      <TableView fx:id="tableView" layoutX="25.0" layoutY="125.0" prefHeight="600.0" prefWidth="750.0">
        <columns>
          <TableColumn fx:id="nameCol" prefWidth="150.0" text="Name" />
          <TableColumn fx:id="qtyCol" prefWidth="150.0" text="Qty" />
            <TableColumn fx:id="expCol" prefWidth="150.0" text="Expiration" />
            <TableColumn fx:id="selectCol" prefWidth="75.0" text="Select" />
            <TableColumn fx:id="recepieCol" prefWidth="75.0" text="Recepie" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="addBtn" layoutX="100.0" layoutY="730.0" mnemonicParsing="false" onAction="#handle" prefHeight="40.0" prefWidth="200.0" text="Add" />
      <Button fx:id="deleteBtn" layoutX="500.0" layoutY="730.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="200.0" text="Delete" />
      <Button fx:id="logOutBtn" layoutX="670.0" layoutY="30.0" mnemonicParsing="false" onAction="#logOutClick" text="Log Out">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
