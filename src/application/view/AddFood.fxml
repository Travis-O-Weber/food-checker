<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="800.0" maxWidth="800.0" prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.AddController">
   <children>
      <Label fx:id="addLabel" alignment="CENTER" layoutX="290.0" layoutY="125.0" text="Add Food">
         <font>
            <Font name="System Bold" size="48.0" />
         </font>
      </Label>
      <TextField fx:id="nameField" layoutX="183.0" layoutY="225.0" prefHeight="50.0" prefWidth="400.0" promptText="Ingredient Name" />
      <TextField fx:id="qtyField" layoutX="183.0" layoutY="325.0" prefHeight="50.0" prefWidth="400.0" promptText="Quantity" />
      <Button fx:id="addBtn" alignment="CENTER" layoutX="258.0" layoutY="522.0" mnemonicParsing="false" onAction="#handle" prefHeight="75.0" prefWidth="250.0" text="Add" />
      <TextField fx:id="expField" layoutX="183.0" layoutY="425.0" minHeight="50.0" minWidth="400.0" prefHeight="50.0" prefWidth="400.0" promptText="Expiration Date (MM/DD/YYY)">
         <font>
            <Font size="27.0" />
         </font>
      </TextField>
      <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#click" text="Back" />
   </children>
</AnchorPane>
